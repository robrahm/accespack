use LaTeXML::Package;
use strict;
# These are all for spacing

DefConstructor('\vertspace', 
	'<ltx:logical-block class="vertspace"></ltx:logical-block>');

DefConstructor('\Vertspace', 
	'<ltx:logical-block class="Vertspace"></ltx:logical-block>');

DefConstructor('\BigVertspace', 
	'<ltx:logical-block class="BigVertspace"></ltx:logical-block>');

DefConstructor('\BigBigVertspace', 
	'<ltx:logical-block class="BigBigVertspace"></ltx:logical-block>');

# These are for boxes for student work
DefConstructor('\workbox{}', 
	'\html{\div aria-label="test"	<ltx:logical-block class="workbox ltx_framed_rectangle">#1</ltx:logical-block>\html{</div>'); 
DefConstructor('\halfworkbox{}', 
	'<ltx:inline-logical-block class="halfworkbox ltx_framed_rectangle">#1</ltx:inline-logical-block>'); 
DefMacro('\bl{}', 
	'\protect\ifmmode\rule[-3.0ex]{#1em}{0.4pt}\else\rule{#1em}{0.4pt}\fi');








1;
